<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="800dp"
    android:height="600dp"
    android:viewportWidth="800"
    android:viewportHeight="600">

  <!-- Background Gradient (Dark Edges, Light Center) -->
  <path
      android:fillColor="#1A1F2B"
  android:pathData="M0,0h800v600h-800z"/>
  <path
      android:fillColor="@color/quantum_white_100"
  android:pathData="M100,100h600v400h-600z"/>

  <!-- Vertical Circuit Lines (Left & Right) -->
  <path
      android:strokeWidth="2"
      android:strokeColor="#00E0FF"
      android:pathData="M50,50v500"/>
  <path
      android:strokeWidth="2"
      android:strokeColor="#00FF88"
      android:pathData="M750,50v500"/>

  <!-- Horizontal Circuit Lines (Top & Bottom) -->
  <path
      android:strokeWidth="2"
      android:strokeColor="#00E0FF"
      android:pathData="M50,50h200"/>
  <path
      android:strokeWidth="2"
      android:strokeColor="#00FF88"
      android:pathData="M750,550h-200"/>

  <!-- Small Circuit Nodes on the Edges -->
  <path
      android:fillColor="#00E0FF"
      android:pathData="M50,50a5,5 0 1,1 50.1,50"/>
  <path
      android:fillColor="#00FF88"
      android:pathData="M750,550a5,5 0 1,1 750.1,550"/>

  <!-- Subtle Circuit-Like Frame (Thin Lines) -->
  <path
      android:strokeWidth="1"
      android:strokeColor="#0099CC"
      android:pathData="M50,100h50v400h-50z"/>
  <path
      android:strokeWidth="1"
      android:strokeColor="#0099CC"
      android:pathData="M700,100h50v400h-50z"/>

</vector>
