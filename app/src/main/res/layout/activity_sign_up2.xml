<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="spontaniius.ui.sign_up.SignUpActivity2">

    <Button
        android:id="@+id/back_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="538dp"
        android:layout_marginEnd="307dp"
        android:layout_marginBottom="3dp"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/done_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="307dp"
        android:layout_marginTop="538dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="3dp"
        android:text="@string/done"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="47dp"
        android:layout_marginBottom="520dp"
        android:text="@string/greeted_string"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/handshake_image"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="145dp"
        android:layout_marginEnd="240dp"
        android:layout_marginBottom="324dp"
        android:src="@drawable/handshake"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/highfive_image"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:layout_marginStart="230dp"
        android:layout_marginTop="145dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="324dp"
        android:src="@drawable/highfive"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/hug_image"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="340dp"
        android:layout_marginEnd="243dp"
        android:layout_marginBottom="90dp"
        android:src="@drawable/hug"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/wave_image"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="78dp"
        android:src="@drawable/wave"
        app:layout_constraintBottom_toTopOf="@+id/done_button2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.366"
        app:layout_constraintStart_toEndOf="@+id/hug_image"
        app:layout_constraintTop_toBottomOf="@+id/highfive_button"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/handshake_button"
        android:layout_width="107dp"
        android:layout_height="26dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="290dp"
        android:layout_marginEnd="254dp"
        android:background="#77FFC107"
        android:text="@string/shake_string"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/hug_button"
        android:layout_width="107dp"
        android:layout_height="26dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="475dp"
        android:layout_marginEnd="258dp"
        android:layout_marginBottom="114dp"
        android:background="#7AFFC107"
        android:text="@string/hug_string"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/highfive_button"
        android:layout_width="107dp"
        android:layout_height="26dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="290dp"
        android:layout_marginEnd="68dp"
        android:background="#72FFC107"
        android:text="@string/five_string"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/wave_button"
        android:layout_width="107dp"
        android:layout_height="26dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="28dp"
        android:background="#63FFC107"
        android:text="@string/wave_string"
        android:textAllCaps="false"
        app:layout_constraintBottom_toTopOf="@+id/done_button2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/hug_button"
        app:layout_constraintTop_toBottomOf="@+id/wave_image"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>
